.content{background:#eeeeec;padding-bottom:50px;}
.inv_banner{height:167px;background:url(../image/inv_banner.jpg) no-repeat center 0;}
.content .center{background:#fff;}
.inv_choose{margin:12px 0 0;border-top:1px #c9c9c9 solid;border-bottom:1px #c9c9c9 solid;height:100px;}
.inv_choose_list{padding-left:22px;overflow:hidden;height:21px;line-height:21px;margin-top:23px;}
.inv_choose_list li{float:left;margin-right:19px;}
.inv_choose_list li.inv_first{margin:0;}
.inv_choose_list li a{text-decoration:none;display:block;padding:0 5px;}
.inv_choose_list li .now,.inv_choose_list li a:hover{color:#fff;background:#a97507;}
.inv_deta{height:40px;margin-top:20px;border-bottom:1px #c9c9c9 solid;padding-left:18px;}
.inv_deta .date_chos{float:left;width:150px;text-align:center;height:38px;line-height:38px;border-top:2px #fff solid;position:relative;bottom:-1px;font-weight:bold;}
.inv_deta .date_chos a{color:#333;display:block;text-decoration:none;}
.inv_deta .date_now{border-left:1px #c9c9c9 solid;border-right:1px #c9c9c9 solid;border-top-color:#bf950d;background:#fff;}
.inv_tab{padding-right:29px;height:29px;line-height:29px;margin-top:2px;}
.inv_tab ul>li{float:left;margin-right:16px;height:27px;border:1px #c9c9c9 solid;padding:0 15px 0 12px;}
.inv_tab ul>li em{color:#a97507;}
.inv_b_box{padding:56px 45px 35px;}
.inv_table{width:1010px;}
.inv_table .table_tr~.table_tr{border-top:1px #e9e9e9 dashed;}
.inv_table th{text-align:center;height:36px;color:#333;font-weight:bold;}
.inv_table td{text-align:center;height:100px;}
.inv_table .table_first{text-align:left;width:240px;}
.inv_table .table_first a{text-decoration:none;}
.inv_table .table_first a:hover{color:#be9320;}
.c_plen{display:inline-block;width:128px;height:8px;background:#dcdcdc;border-radius:12px;}
.c_plen>.speed{display:block;width:0;height:8px;border-radius:12px;background:#d4a50b url(/2015/index/image/speed_bg0918.jpg) repeat;}
.inv_table .num{display:block;color:#9a6b38;font-family:"微软雅黑";line-height:14px;}
.inv_table .detail_a{display:inline-block;width:116px;height:38px;text-align:center;border-radius:7px;background:url(../image/c_btn_bg2.png) no-repeat center 0;color:#a25e23;font:18px/38px "微软雅黑";text-decoration:none;}


.inv_table .detail_a:hover{background:url(../image/c_btn_bg2h.png) no-repeat center 0;}

.inv_table .inv_com{background:#d3d3d3;color:#666;}